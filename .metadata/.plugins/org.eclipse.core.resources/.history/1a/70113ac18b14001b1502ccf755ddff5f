package br.com.curso.modelo;

public class Curso {

	private int id;
	private String descricao;
	private float valor;
	private int cargaHoraria;
	
	public Curso() {
		super();
	}

	public Curso(int id, String descricao, float valor, int cargaHoraria) {
		super();
		this.id = id;
		this.descricao = descricao;
		this.valor = valor;
		this.cargaHoraria = cargaHoraria;
	}
	
	public void setALL(int id, String descricao, float valor, int cargaHoraria ) {
		this.id = id;
		this.descricao = descricao;
		this.valor = valor;
		this.cargaHoraria = cargaHoraria;
				
	}
	
	public void getAll(int id, String descricao, float valor, int cargaHoraria) {
		this.id = id;
		this.descricao = descricao;
		this.valor = valor;
		this.cargaHoraria = cargaHoraria;
						
	}
	
	
	
	
	
	
	
	
	
	
	
}// Classe
