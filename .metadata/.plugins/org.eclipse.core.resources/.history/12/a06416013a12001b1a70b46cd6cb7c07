package variaveis;

import javax.swing.JOptionPane;

public class Notas {

	public static void main(String[] args) {
		String nome = JOptionPane.showInputDialog("Digite seu Nome");
		String diciplina = JOptionPane.showInputDialog("Digite Diciplina");
		double nota1 = Double.parseDouble(JOptionPane.showInputDialog("Digite Nota1"));
		double nota2 = Double.parseDouble(JOptionPane.showInputDialog("Digite Nota2"));
		System.out.println("Aluno:.:"+nome);
		System.out.println("Diciplina...................:" + diciplina);
		System.out.printf("Media:.....: + (nota1+nota2)%.2f");

	}

}
