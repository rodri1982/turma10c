package br.com.universidade.modelo;

public class Bacharelado {
	private int cargaHoraria;
	private String trabalhoConclusao;
	
	
	public String getAll() {
		return
				"Carga Horária........: " + cargaHoraria + "\n" +
				"Trabalho Conclusao...: " + trabalhoConclusao;
		
				
	}
	
	
	
	public void setAll(int cargaHoraria, String trabalhoConclusao) {
		this.cargaHoraria = cargaHoraria;
		this.trabalhoConclusao = trabalhoConclusao;
	}
	public Bacharelado(int cargaHoraria, String trabalhoConclusao) {
		super();
		this.cargaHoraria = cargaHoraria;
		this.trabalhoConclusao = trabalhoConclusao;
	}
	public Bacharelado() {
		super();
	}
	public int getCargaHoraria() {
		return cargaHoraria;
	}
	public void setCargaHoraria(int cargaHoraria) {
		this.cargaHoraria = cargaHoraria;
	}
	public String getTrabalhoConclusao() {
		return trabalhoConclusao;
	}
	public void setTrabalhoConclusao(String trabalhoConclusao) {
		this.trabalhoConclusao = trabalhoConclusao;
	}
	
	
	
	
	
	
	

}
