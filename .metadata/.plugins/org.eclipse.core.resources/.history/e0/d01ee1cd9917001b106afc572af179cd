package br.com.colecoes.beans;

public class Cargo {

	public class cargo {
		private String nome;
		private String nivel;
		private String Salario;
		
		
		
		public cargo() {
			super();
		}
		
		
		
		
		public cargo(String nome, String nivel, String salario) {
			super();
			this.nome = nome;
			this.nivel = nivel;
			Salario = salario;
		}




		public String getNome() {
			return nome;
		}
		public void setNome(String nome) {
			this.nome = nome;
		}
		public String getNivel() {
			return nivel;
		}
		public void setNivel(String nivel) {
			this.nivel = nivel;
		}
		public String getSalario() {
			return Salario;
		}
		public void setSalario(String salario) {
			Salario = salario;
		}
		

		
		
	}
}
